html
  head
    link(rel="stylesheet" href="css/pagestyle.css")
  body
    
    //canvas(id="mycanvas", width="422", height="547")
    script(src="js/draw.js")
    
    div#data
      
      h1#word= word
      ul#definitions
        li= def1
        li= def2
        li= def3

      ul#synonyms
        for synonym in synonyms
          li= synonym

      ul#antonyms
        for antonym in antonyms
          li= antonym

      ul#partsofspeech
        for part in pos
          li= part

      ul#sentences
        for sentence in sentences
          li= sentence